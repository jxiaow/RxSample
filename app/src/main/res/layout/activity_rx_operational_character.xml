<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="cn.xwj.rxsample.RxOperationalCharacterActivity">

    <cn.xwj.easy.widget.TitleView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorPrimary"
        app:title_text="操作符" />

    <Button
        android:id="@+id/btn_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Map操作符" />

    <Button
        android:id="@+id/btn_flat_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="FlatMap操作符" />

    <Button
        android:id="@+id/btn_concat_map"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="concatMap操作符" />

    <Button
        android:id="@+id/btn_zip"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="zip操作符" />

    <TextView
        android:id="@+id/result"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
</LinearLayout>
